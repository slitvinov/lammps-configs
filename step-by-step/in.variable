include "in.minsetup"

# simple variable
variable q equal 10
print "q=$q"
variable q delete

# index variable
variable x index run1 run2 run3 run4 run5 run6 run7 run8
print "x=$x"
next x
print "x=$x"
variable x delete

# loop variable
variable LoopVar loop 10
print "LoopVar=${LoopVar}"
next LoopVar
print "LoopVar=${LoopVar}"
variable LoopVar delete

# uloop variable
label loop
variable ULoopVar uloop 5
print "ULoopVar = ${ULoopVar}"
next ULoopVar
jump SELF loop
print "end of the uloop"
